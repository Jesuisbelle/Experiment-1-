#include <iostream>
#include <conio.h>
using namespace std;
int main ()
{

		cout << "Type:		Ticket Price:	Number sold:	Total:";
		cout << "Box:	250				5750			"<< 250 * 5750 << endl;
		cout << "Sideline:	100				2800		"<< 100 * 2800 << endl;
		cout << "Premium:	50				35750		"<< 50 * 35750 << endl;
		cout << "General:	25				18750		"<< 25 * 18750 << endl;

	_getch();
	return 0;
}
